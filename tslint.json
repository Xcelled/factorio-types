{
    "extends": "tslint:recommended",
    "rules": {
        "semicolon": [true, "always", "ignore-interfaces"],
        "interface-name": false,
        "class-name": false,
        "no-namespace": [true, "allow-declarations"],
        "quotemark": [true, "single"],
        "variable-name": false,
        "member-ordering": false,
        "no-empty-interface": false
    }
}